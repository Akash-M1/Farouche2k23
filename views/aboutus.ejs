<main class="page projets-page">
    <section class="portfolio-block project-no-images">
        <div class="container">
            <div class="heading" >
                <h2><%= heading %></h2>
            </div>
            <div class="row">

                <% for( let index = 0; index < array.length; index++ ) { %>
                    <div class="col-md-6 col-lg-4">
                        <div class="project-card-no-image" style="position: relative; ">
                            <table>
                                <tr>
                                    <td>
                                        <h3><%= array[index].name %></h3>
                                        <h4><%= array[index].work %></h4>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="pos-img"><img class="img-about" src="<%= array[index].picture %>" alt=""></td>
                                </tr>
                            </table>
                        </div>
                    </div>
                <% } %>
            </div>
        </div>
    </section>
    
</main>